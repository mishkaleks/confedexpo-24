  "scripts": {
    "start-prod": "better-npm-run start-prod",
    "lint": "eslint -c .eslintrc src",
    "dev": "better-npm-run start-dev",
    "dev-cypress": "better-npm-run dev-cypress",
    "prod-cypress": "better-npm-run prod-cypress",
    "test-cypress": "npx cypress open --config baseUrl=https://test.screver.com --env isTest=true",
    "build": "next build",
    "analyze": "ANALYZE=true next build"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "dev-cypress": {
      "command": "node ./server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "CYPRESS": true,
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "prod-cypress": {
      "command": "node ./server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "CYPRESS": true,
        "PORT": 3000,
        "APIPORT": 3030
      }
    }
  },
